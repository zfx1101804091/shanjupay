<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shanjupay.merchant.mapper.StoreMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.shanjupay.merchant.entity.Store">
        <id column="ID" property="id"/>
        <result column="STORE_NAME" property="storeName"/>
        <result column="STORE_NUMBER" property="storeNumber"/>
        <result column="MERCHANT_ID" property="merchantId"/>
        <result column="PARENT_ID" property="parentId"/>
        <result column="STORE_STATUS" property="storeStatus"/>
        <result column="STORE_ADDRESS" property="storeAddress"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, STORE_NAME, STORE_NUMBER, MERCHANT_ID, PARENT_ID, STORE_STATUS, STORE_ADDRESS
    </sql>

</mapper>
